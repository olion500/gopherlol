{
  "commands": [
    {
      "name": "google",
      "aliases": ["g", "search"],
      "description": "Search Google",
      "url": "https://www.google.com/search?q={{.Query}}",
      "requiresQuery": true,
      "default": true
    },
    {
      "name": "github",
      "aliases": ["gh"],
      "description": "GitHub operations",
      "url": "https://github.com/search?q={{.Query}}",
      "requiresQuery": false,
      "subcommands": [
        {
          "name": "pr",
          "aliases": ["pull", "pullrequest"],
          "description": "Search GitHub pull requests",
          "url": "https://github.com/search?type=pullrequests&q={{.Query}}"
        },
        {
          "name": "issues",
          "aliases": ["issue"],
          "description": "Search GitHub issues",
          "url": "https://github.com/search?type=issues&q={{.Query}}"
        },
        {
          "name": "repo",
          "aliases": ["repository"],
          "description": "Search GitHub repositories",
          "url": "https://github.com/search?type=repositories&q={{.Query}}"
        },
        {
          "name": "user",
          "aliases": ["users"],
          "description": "Search GitHub users",
          "url": "https://github.com/search?type=users&q={{.Query}}"
        },
        {
          "name": "trends",
          "aliases": ["tr", "trend"],
          "description": "GitHub trending repositories",
          "url": "https://github.com/trending"
        }
      ]
    },
    {
      "name": "stackoverflow",
      "aliases": ["so", "stack"],
      "description": "Search Stack Overflow",
      "url": "https://stackoverflow.com/search?q={{.Query}}",
      "requiresQuery": true
    },
    {
      "name": "youtube",
      "aliases": ["yt"],
      "description": "Search YouTube",
      "url": "https://www.youtube.com/results?search_query={{.Query}}",
      "requiresQuery": true
    },
    {
      "name": "twitter",
      "aliases": ["tw", "x"],
      "description": "Search Twitter/X",
      "url": "https://twitter.com/search?q={{.Query}}",
      "requiresQuery": true
    },
    {
      "name": "gmail",
      "aliases": ["mail", "email", "gm"],
      "description": "Gmail search",
      "url": "https://mail.google.com/mail/#search/{{.Query}}",
      "requiresQuery": false
    },
    {
      "name": "calendar",
      "aliases": ["cal"],
      "description": "Google Calendar",
      "url": "https://calendar.google.com/calendar/u/0/r",
      "requiresQuery": false
    },
    {
      "name": "jira",
      "aliases": ["j"],
      "description": "Open Jira ticket by key (customize URL for your instance)",
      "url": "https://yourcompany.atlassian.net/browse/{{.Query}}",
      "requiresQuery": true,
      "subcommands": [
        {
          "name": "board",
          "aliases": ["kanban"],
          "description": "Open your Jira board",
          "url": "https://yourcompany.atlassian.net/jira/software/projects/YOUR_PROJECT/boards/1",
          "requiresQuery": false
        }
      ]
    },
    {
      "name": "slack",
      "aliases": ["sl"],
      "description": "Slack operations (customize for your workspace)",
      "url": "https://yourworkspace.slack.com/",
      "requiresQuery": false,
      "subcommands": [
        {
          "name": "general",
          "description": "General channel",
          "url": "https://yourworkspace.slack.com/app_redirect?channel=general",
          "requiresQuery": false 
        }
      ]
    },
    {
      "name": "vscode",
      "aliases": ["code", "vs"],
      "description": "Open file/folder in VS Code",
      "url": "vscode://{{.Query}}",
      "requiresQuery": false
    }
  ]
}